{
  "name": "go-annotext-graph",
  "version": "1.0.2",
  "description": "GO annotation extension graph",
  "main": "index.ts",
  "author": "Xavier Watkins <xwatkins@ebi.ac.uk>",
  "license": "MIT",
  "scripts": {
    "start": "rollup -cw | browser-sync start --server --index='./public/index.html' --files='./dist/*.js,./public/*.html'",
    "compile": "rollup -c",
    "transpile": "babel dist -d build",
    "bundle": "browserify ./build/go-annotext-graph.js -o ./build/bundle.js",
    "build": "npm run compile && npm run transpile && npm run bundle"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "d3": "^5.9.1",
    "d3-ellipse-force": "^0.1.1",
    "d3-scale-chromatic": "^1.3.3",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/d3-drag": "^1.2.2",
    "@types/d3-force": "^1.2.0",
    "@types/d3-scale-chromatic": "^1.3.0",
    "@types/d3-selection": "^1.4.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.3"
  }
}
